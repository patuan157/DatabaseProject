{
  "author": {
    "authid": "int",
    "name": "text",
  },
  "authored": {
    "authid": "int REFERENCES author(authid)",
    "authname": "text",
    "pubkey": "varchar(100) REFERENCES publication(pubkey)",
  },
  "publication": {
    "pubkey": "varchar(100)",
    "pubtype": "varchar(20)", // article or inproceedings
    "title": "text",
    "mdate": "date",
    "year": "int",
    "numberauthor": "int default 0",
  },
  "article": {
    "akey": "varchar(100) REFERENCES publication(pubkey)",
    "mdate": "date",
    "year": "int",
    "numberauthor": "int defaut 0",
    "title": "text",
    "journal_code": "String"
  },
  "inproceedings": {
    "ikey": "varchar(100) REFERENCES publication(pubkey)",
    "mdate": "date",
    "year": "int",
    "numberauthor": "int DEFAULT 0",
    "title": "text",
    "conf_code": "varchar(100)",
    "decade": "varchar(4)"
  },
  "conference": {       // Extract Data as it is Proceeding
    "conferenceid": "int",
    "conf_code": "varchar(100)",
    "publisher": "text",
    "title": "text",
    "year": "int",
    "month": "int",
  },
  "journal": {          // Only Article Appear
    "journalid": "int",
    "journal_code": "varchar(100)",
    "title": "text",
    "year": "int",
    "month": "int",
  }
}