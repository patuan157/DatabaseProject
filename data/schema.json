{
  "author": {
    "id": "text primary key", // full name
    "family_name": "text",
    "given_name": "text not null"
  },
  "link_publ_auth": {
    "id": "int serial primary key",
    "auth_id": "text REFERENCES author(id)",
    "publ_id": "text REFERENCES publication(id)"
  },
  "publication": {
    "id": "text primary key",
    "title": "text not null",
    "mdate": "date not null",
    "year": "int not null",
    "no_authors": "int not null",
    "decade": "varchar(9) not null"
  },
  "article": {
    "id": "varchar(100) REFERENCES publication(pubkey)",
    "journal_id": "text not null"
  },
  "inproceedings": {
    "id": "varchar(100) REFERENCES publication(pubkey)",
    "conf_id": "text not null"
  },
  "conference": {       // Extract Data as it is Proceeding
    "id": "int serial primary key",
    "code": "varchar(100) not null",
    "title": "text not null",
    "year": "int not null",
    "month": "int not null"
  },
  "journal": {          // Only Article Appear
    "id": "int serial primary key",
    "code": "varchar(100) not null",
    "title": "text not null",
    "year": "int not null",
    "month": "int not null"
  }
}
